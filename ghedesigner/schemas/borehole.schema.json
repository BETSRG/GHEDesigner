{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "buried_depth": {
      "type": "number",
      "minimum": 0,
      "format": "Meters",
      "description": "Depth of top of borehole below the ground surface."
    },
    "diameter": {
      "type": "number",
      "format": "Meters",
      "description": "Diameter of borehole."
    }
  },
  "required": [
    "buried_depth",
    "diameter"
  ]
}
