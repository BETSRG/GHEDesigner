{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "conductivity": {
      "type": "number",
      "minimum": 0,
      "format": "Watts/Meter-Kelvin",
      "description": "Thermal conductivity."
    },
    "rho_cp": {
      "type": "number",
      "minimum": 0,
      "format": "Joules/Meter^3-Kelvin",
      "description": "Volumetric heat capacity."
    }
  },
  "required": ["conductivity", "rho_cp"]
}
